
public class Solution {
	public boolean Find(int[][] array, int target){
		if(array.length>0){
			int i=0;
			int j=array[i].length-1;
			while((i<array.length)&&(j>=0)){
				if(array[i][j]==target) return true;
				else if(array[i][j]>target) j--;
				else if(array[i][j]<target) i++;
			}
		}
		
		return false;
	}
}
