import java.util.Arrays;

/*
 * base on the merge sort
 */
public class Solution {
	public int getNumberOfInversePairs(int[] array){
		if(array.length<=1) return 0;
		int[] copy = Arrays.copyOf(array, array.length);
		return getNumberOfInversePairsRecu(array, copy, 0, array.length-1);
	}
	
	private int getNumberOfInversePairsRecu(int[] array, int[] copy, int start, int end){
		
	}
}
