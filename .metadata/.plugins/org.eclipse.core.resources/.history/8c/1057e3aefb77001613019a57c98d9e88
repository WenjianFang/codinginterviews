
public class Solution {
	public ListNode reverseList(ListNode head){
		if(head == null) return null;
		if(head.next == null) return head;
		
		ListNode p = head.next;
		ListNode next;
		
		while(p.next != null){
			next = p.next;
			p.next = head;
			p = next;
		}
		p.next = head;
		head.next = null;
		return p;
	}
}